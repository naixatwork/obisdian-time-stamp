{
	"name": "obsidian-dashboard",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"npm run tailwind:watch\" \"node esbuild.config.mjs\"",
		"build": "concurrently \"npm run tailwind\" \"tsc -noEmit -skipLibCheck && node esbuild.config.mjs production\"",
		"test:watch": "jest --watch --verbose",
		"lint:fix": "eslint --fix . --ext .ts,tsx",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"tailwind:watch": "npx tailwindcss -i ./src/main.css -o ./styles.css --watch",
		"tailwind": "npx tailwindcss -i ./src/main.css -o ./styles.css",
		"test:publish": "jest --verbose",
		"lint": "eslint . --ext .ts,tsx"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@stryker-mutator/core": "^7.1.1",
		"@stryker-mutator/jest-runner": "^7.1.1",
		"@tailwindcss/container-queries": "^0.1.1",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@types/jest": "^29.5.3",
		"@types/lodash": "^4.14.196",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.17",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"concurrently": "^8.2.0",
		"esbuild": "0.17.3",
		"eslint": "^8.45.0",
		"jest": "^29.6.2",
		"jest-environment-jsdom": "^29.6.2",
		"obsidian": "latest",
		"tailwindcss": "^3.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@codemirror/view": "^6.15.3",
		"@preact/signals-react": "^1.3.4",
		"lodash": "^4.17.21",
		"lucide-react": "^0.263.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"tsyringe": "^4.8.0"
	}
}
