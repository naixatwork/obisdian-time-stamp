{
	"name": "obsidian-dashboard",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "concurrently \"npm run tailwind:watch\" \"node esbuild.config.mjs\"",
		"tailwind:watch": "npx tailwindcss -i ./src/main.css -o ./styles.css --watch",
		"tailwind": "npx tailwindcss -i ./src/main.css -o ./styles.css",
		"build": "concurrently \"npm run tailwind\" \"tsc -noEmit -skipLibCheck && node esbuild.config.mjs production\"",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.17",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"concurrently": "^8.2.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tailwindcss": "^3.3.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@codemirror/view": "^6.15.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"tsyringe": "^4.8.0"
	}
}
